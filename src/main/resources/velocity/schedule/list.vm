#parse("/velocity/layout.vm")
#@mainLayout()
  #parse("/velocity/schedule/script.vm")
<h4>Schedule Management</h4>
<br/>
<a href='#' onclick="generate()" class="btn btn-info">Generate</a>
<a href='#' onclick="reset()" class="btn btn-danger">Reset</a>
<div class="col-md-15">
  <br>
  <div id="message"></div>
  <table id="datatable" class=" table table-hover" style="width:100%">
    <thead>
    <tr>
      <th class="td">Block</th>
      <th class="td">Date</th>
      <th class="td">Courses</th>
    </tr>
    </thead>
    <tbody>
      #foreach($block in $blocks)
      <tr>
        <td class="td"> $block.getId()</td>
        <td class="td">
          $block.getName()
        </td>
        <td class="td"> $block.getStartDate()</td>
        <td class="td"> $block.getEndDate()</td>

        <td class="td">
          <a href="#" onclick="getblock($block.getId())">
            <i class="glyphicon glyphicon-edit"></i> </a>

          <a href="#" onclick="deleteblock($block.getId())">
            <i class="glyphicon glyphicon-remove"></i> </a>
        </td>

      </tr>
      #end
    </tbody>
  </table>
</div>
#end
